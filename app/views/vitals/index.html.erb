<p id="notice"><%= notice %></p>

<h1>Listing Vitals</h1>

<table>
  <thead>
    <tr>
      <th>Height</th>
      <th>Weight</th>
      <th>HeadCircumference</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vitals.each do |vital| %>
      <tr>
        <td><%= vital.Height %></td>
        <td><%= vital.Weight %></td>
        <td><%= vital.HeadCircumference %></td>
        <td><%= vital.Date %></td>
        <td><%= link_to 'Show', vital %></td>
        <td><%= link_to 'Edit', edit_vital_path(vital) %></td>
        <td><%= link_to 'Destroy', vital, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vital', new_vital_path %>
